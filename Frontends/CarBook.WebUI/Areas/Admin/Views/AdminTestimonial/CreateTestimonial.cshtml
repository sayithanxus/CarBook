@model CreateTestimonialDto
@{
    ViewData["Title"] = "CreateTestimonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<div class="page-content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">

                        <h4 class="card-title">Yeni Referans Bilgisi Girişi</h4>
                        <form method="post">
                            <div class="form-group">
                                <label for="exampleInputName">Adı Soyadı</label>
                                <input type="text" class="form-control" id="exampleInputName" asp-for="name">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputTitle">Başlık</label>
                                <input type="text" class="form-control" id="exampleInputTitle" asp-for="title">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputComment">Yorum</label>
                                <input type="text" class="form-control" id="exampleInputComment" asp-for="comment">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputImageUrl">Resim URL</label>
                                <input type="text" class="form-control" id="exampleInputImageUrl" asp-for="imageUrl">
                            </div>
                            <button type="submit" class="btn btn-primary waves-effect waves-light">Kaydet</button>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>








